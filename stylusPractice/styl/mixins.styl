vendor(property, arg)
    {property} arg
    -webkit-{property} arg
    -moz-{property} arg

transition(args)
    vendor('transition',args)

border-radius(radius=5px)
    vendor('border-radius',radius)
cf()
    zoom 1
    &:after
    &:before
        content ""
        display table
    &:after
        clear both

